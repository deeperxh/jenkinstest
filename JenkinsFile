 node {
     stage('SCM') {
         git 'https://github.com/deeperxh/jenkinstest.git'
     }

     stage('build') {
         sh 'mvn -B clean package'
     }

     stage('deploy') {
         sh 'java -jar target/jenkinsdemo.jar'
     }

     stage('results') {
         archiveArtifacts artifacts: '**/target/*.war', fingerprint: true
     }
 }